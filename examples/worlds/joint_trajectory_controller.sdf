            <ambient>0.5 0.5 0.5 1</ambient>
            <diffuse>0.8 0.8 0.8 1</diffuse>
            <specular>0.8 0.8 0.8 1</specular>
          </material>
        </visual>
      </link>
      <link name="RR_velocity_control_link1">
        <pose relative_to="RR_velocity_control_joint1">0 0 0.1 0 0 0</pose>
