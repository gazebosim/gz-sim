<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="default">
    <plugin
      filename="libignition-gazebo1-physics-system.so"
      name="ignition::gazebo::systems::Physics">
    </plugin>
    <plugin
      filename="libignition-gazebo1-scene-broadcaster-system.so"
      name="ignition::gazebo::systems::SceneBroadcaster">
    </plugin>

    <gui fullscreen="0">

      <!-- 3D scene -->
      <plugin filename="Scene3D" name="3D View">
        <ignition-gui>
          <title>3D View</title>
          <property type="bool" key="showTitleBar">false</property>
          <property type="string" key="state">docked</property>
        </ignition-gui>

        <engine>ogre</engine>
        <scene>scene</scene>
        <ambient_light>0.4 0.4 0.4</ambient_light>
        <background_color>0.8 0.8 0.8</background_color>
        <camera_pose>1 -2 1.2 0 0.5 2</camera_pose>
        <service>/world/default/scene/info</service>
        <pose_topic>/world/default/pose/info</pose_topic>
      </plugin>

      <!-- World control -->
      <plugin filename="WorldControl" name="World control">
        <ignition-gui>
          <title>World control</title>
          <property type="bool" key="showTitleBar">false</property>
          <property type="bool" key="resizable">false</property>
          <property type="double" key="height">72</property>
          <property type="double" key="width">121</property>
          <property type="double" key="z">1</property>

          <property type="string" key="state">floating</property>
          <anchors target="3D View">
            <line own="left" target="left"/>
            <line own="bottom" target="bottom"/>
          </anchors>
        </ignition-gui>

        <play_pause>true</play_pause>
        <step>true</step>
        <start_paused>true</start_paused>
        <service>/world/default/control</service>
        <stats_topic>/world/default/stats</stats_topic>

      </plugin>
    </gui>
    <model name="ground_plane">
      <static>true</static>
      <link name="link">
        <pose>0 0 -0.5 0 0 0</pose>
        <collision name="collision">
          <geometry>
            <box>
              <size>100 100 1</size>
            </box>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>100</mu>
                <mu2>50</mu2>
              </ode>
            </friction>
          </surface>
        </collision>
        <visual name="visual">
          <cast_shadows>false</cast_shadows>
          <geometry>
            <box>
              <size>100 100 1</size>
            </box>
          </geometry>
          <material>
            <ambient>1 1 1 1</ambient>
            <diffuse>1 1 1 1</diffuse>
            <specular>1 1 1 1</specular>
          </material>
        </visual>
      </link>
    </model>
    <model name="demo_joint_types">
        <!-- A Heavy base keeps the joints from falling over -->
        <link name="heavy_base">
            <!-- position the base such that the bottom touches the floor -->
            <pose>0 0 0.05 0 0 0</pose>
            <inertial>
                <mass>100</mass> <!-- kg -->
                <inertia>
                    <!-- (mass / 12) * (y*y + z*z) -->
                    <ixx>8.4167</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <!-- (mass / 12) * (x*x + z*z) -->
                    <iyy>40.417</iyy>
                    <iyz>0</iyz>
                    <!-- (mass / 12) * (y*y + x*x) -->
                    <izz>48.667</izz>
                </inertia>
            </inertial>
            <!-- Same geometry as visual -->
            <collision name="heavy_base_collision">
                <geometry>
                    <box>
                        <size>2.2 1 0.1</size>
                    </box>
                </geometry>
            </collision>
            <!-- Same geometry as collision -->
            <visual name="heavy_base_visual">
                <geometry>
                    <box>
                        <size>2.2 1 0.1</size>
                    </box>
                </geometry>
                <material>
                  <ambient>0 0 1 1</ambient>
                  <diffuse>0 0 1 1</diffuse>
                  <specular>0 0 1 1</specular>
                </material>
            </visual>
        </link>


        <!-- ****************************************
             revolute joint demo
             **************************************** -->
        <link name="revolute_base">
            <!-- Put the bottom of the child on top of the parent
                 z = 0.5 * revolute_base_sz_z + heavy_base_sz_z
                 x = put it near revolute label on heavy base
                 -->
            <pose >-.72 0 0.35 0 0 0</pose>
            <inertial>
                <inertia>
                    <!-- (mass / 12) * (y*y + z*z) -->
                    <ixx>0.021667</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <!-- (mass / 12) * (x*x + z*z) -->
                    <iyy>0.021667</iyy>
                    <iyz>0</iyz>
                    <!-- (mass / 12) * (y*y + x*x) -->
                    <izz>0.0016667</izz>
                </inertia>
            </inertial>
            <!-- Same geometry as visual -->
            <collision name="revolute_base_collision">
                <geometry>
                    <box>
                        <size>0.1 0.1 0.5</size>
                    </box>
                </geometry>
            </collision>
            <!-- Same geometry as collision -->
            <visual name="revolute_base_visual">
                <geometry>
                    <box>
                        <size>0.1 0.1 0.5</size>
                    </box>
                </geometry>
                <material>
                  <ambient>0.7 0.7 0.7 1</ambient>
                  <diffuse>0.7 0.7 0.7 1</diffuse>
                  <specular>0.7 0.7 0.7 1</specular>
                </material>
            </visual>
        </link>

        <joint name="revolute_base_to_heavy_base" type="fixed">
            <parent>heavy_base</parent>
            <child>revolute_base</child>
        </joint>

        <link name="revolute_arm">
            <!-- Put the arm near the top of the revolute_base
                 z = revolute_base_sz_z + heavy_base_sz_z - 0.5 * revolute_arm_sz_z
                 y = -0.5 * revolute_arm_sz_y + fudge(0.05)
                 x = revolute_base_x - 0.5 * revolute_base_sz_x + 0.5 * revolute_arm_sz_x + fudge(0.01)
                 -->
            <pose >-.61 -0.075 0.55 0 0 0</pose>
            <inertial>
                <inertia>
                    <!-- (mass / 12) * (y*y + z*z) -->
                    <ixx>0.0060417</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <!-- (mass / 12) * (x*x + z*z) -->
                    <iyy>0.0016667</iyy>
                    <iyz>0</iyz>
                    <!-- (mass / 12) * (y*y + x*x) -->
                    <izz>0.0060417</izz>
                </inertia>
            </inertial>
            <!-- Same geometry as visual -->
            <collision name="revolute_arm_collision">
                <geometry>
                    <box>
                        <size>0.1 0.25 0.1</size>
                    </box>
                </geometry>
            </collision>
            <!-- Same geometry as collision -->
            <visual name="revolute_arm_visual">
                <geometry>
                    <box>
                        <size>0.1 0.25 0.1</size>
                    </box>
                </geometry>
                <material>
                  <ambient>1 0 0 1</ambient>
                  <diffuse>1 0 0 1</diffuse>
                  <specular>1 0 0 1</specular>
                </material>
            </visual>
        </link>

        <joint name="revolute_demo" type="revolute">
            <parent>revolute_base</parent>
            <child>revolute_arm</child>
            <axis>
                <xyz>1 0 0</xyz>
                <limit>
                  <lower>-3.14</lower>
                  <upper>3.14</upper>
                </limit>
            </axis>
            <!-- Move the joint to the size a bit to be more like an arm
                 y = 0.5 * relative_arm_sz_y - fudge(0.05) -->
            <pose>0 0.075 0 0 0 0</pose>
        </joint>

