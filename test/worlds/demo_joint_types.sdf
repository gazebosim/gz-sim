             ball joint demo
             **************************************** -->
        <link name="ball_socket">
            <!-- make the socket float in the sky -->
            <pose >0.4 0 0.55 0 0 0</pose>
            <inertial>
                <inertia>
                    <!-- (mass / 12) * (y*y + z*z) -->
                    <ixx>0.0016667</ixx>
