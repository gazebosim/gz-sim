             gearbox joint demo
             **************************************** -->
        <link name="gearbox_base">
            <!-- Put the bottom of the child on top of the parent
                 z = 0.5 * gearbox_base_sz_z + heavy_base_sz_z
                 x = put it near gearbox label on heavy base
                 -->
            <pose >-.49 0 0.35 0 0 0</pose>
            <inertial>
                <inertia>
                    <!-- (mass / 12) * (y*y + z*z) -->
                    <ixx>0.021667</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <!-- (mass / 12) * (x*x + z*z) -->
                    <iyy>0.021667</iyy>
                    <iyz>0</iyz>
                    <!-- (mass / 12) * (y*y + x*x) -->
                    <izz>0.0016667</izz>
                </inertia>
            </inertial>
            <!-- Same geometry as visual -->
