      <joint name='left_wheel_joint_green' type='revolute'>
        <parent>chassis_green</parent>
        <child>left_wheel_green</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1.79769e+308</lower>
