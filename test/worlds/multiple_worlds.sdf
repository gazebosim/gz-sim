      <link name="link">
        <collision name="col">
          <geometry>
            <sphere>
              <radius>23.4</radius>
            </sphere>
          </geometry>
        </collision>

        <visual name="vis">
          <geometry>
            <sphere>
              <radius>100.2</radius>
            </sphere>
          </geometry>
        </visual>
      </link>
