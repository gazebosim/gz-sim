    <model name="cylinder">
      <pose>-1 -2 -3 0 0 1</pose>
      <link name="link">
        <collision name="col">
          <geometry>
            <cylinder>
              <radius>0.2</radius>
              <length>0.1</length>
            </cylinder>
          </geometry>
        </collision>

        <visual name="vis">
          <geometry>
            <cylinder>
              <radius>2.1</radius>
              <length>10.2</length>
            </cylinder>
          </geometry>
        </visual>
      </link>
    </model>
